############################################################################
# VPC
############################################################################
resource "aws_vpc" "vpc" {
  cidr_block = "10.0.0.0/16"
  tags = {
    "Name" = "example-vpc"
  }
}

############################################################################
# InternetGateway
############################################################################
resource "aws_internet_gateway" "igw" {
  tags = {
    "Name" = "example-igw"
  }
  vpc_id = aws_vpc.vpc.id
}
